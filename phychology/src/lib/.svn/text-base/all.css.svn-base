@charset "utf-8";
/* CSS Document */

/*全局样式*/
html {
    _background: url(about:blank);
}

body {
    background: #eeeeee;
}

html, body {
    margin: 0;
    padding: 0;
}

* {
    -webkit-appearance: none;
}

a:visited,
a {
    text-decoration: none;
    cursor: pointer;
    color: #333;
}

input[type="checkbox"] {
    -webkit-appearance: checkbox;
}

input[type="radio"] {
    -webkit-appearance: radio;
    vertical-align: sub;
    margin-right: 3px;
}

body, html, h1, h2, h3, h4, p, ul, li, form, ol {
    margin: 0;
    padding: 0;
}

ul {
    list-style: none;
}

body, input, textarea, select {
    font-size: 14px;
    font-family: "微软雅黑", arail, sans-sarif;
    color: #333;
}

i {
    display: inline-block;
    vertical-align: middle;
}

.clearfix {
    *zoom: 1;
}

.clearfix:after {
    content: '\0020';
    display: block;
    height: 0;
    font-size: 0;
    clear: both;
    overflow: hidden
}

.left {
    float: left;
}

.right {
    float: right;
}

.clear {
    clear: both;
}

img {
    border: none;
    vertical-align: middle
}

.tl {
    text-align: left;
}

.tc {
    text-align: center;
}

.tr {
    text-align: right;
}

.mr10 {
    margin-right: 10px;
}

.mt60 {
    margin-top: 60px;
}

.mb8 {
    margin-bottom: 8px;
}

.ptb15 {
    padding: 15px 0;
}

.pt20 {
    padding-top: 20px;
}

.pt30 {
    padding-top: 30px;
}

.bd_bot {
    border-bottom: 1px solid #d1d1d1;
}

.bd_top {
    border-bottom: 1px solid #d1d1d1;
}

input, button, select, textarea {
    outline: none
}

textarea {
    resize: none;
}

.wrap {
    margin: auto;
    position: relative;
    min-width: 320px;
    max-width: 640px;
    min-height: 480px;
    margin-top: 44px;
    height: 100%;
}

.header {
    background: #29282d;
    color: #fff; /* border-bottom:1px solid #d1d1d1;*/
    text-align: center;
    font-size: 16px;
    height: 44px;
    line-height: 44px;
    position: fixed;
    z-index: 9999;
    left: 0;
    top: 0;
    width: 100%;
}

.header_in {
    position: relative;
    padding: 0 11px 0 11px;
}

.Btn_return {
    position: absolute;
    left: 11px;
    top: 18px;
    display: inline-block;
    background: url(../images/header_pre.png) no-repeat left center;
    background-size: 9px 16px;
    width: 9px;
    height: 16px;
}

.Btn_function {
    position: absolute;
    right: 11px;
    top: 19px;
    display: inline-block;
    background: url(../images/Btn_function.png) no-repeat left center;
    background-size: 22px 5px;
    width: 22px;
    height: 5px;
}

.box {
    padding: 0 11px;
    border-bottom: 1px solid #d1d1d1;
    background: #fff;
}

.bd_none {
    border-bottom: none
}

.section_box {
    padding: 0 11px;
    border-bottom: 1px solid #d1d1d1;
    background: #fff;
    position: relative;
    z-index: 1000;
}

.section_nav {
    width: 100%;
    height: 50px;
    line-height: 50px;
    position: relative;
}

.section_nav .address {
    position: relative;
    display: inline-block;
}

.input_address {
    font-size: 14px;
    color: #9e9e9e;
    background: url(../images/arro_down.png) no-repeat right center;
    background-size: 16px 9px;
    padding-right: 22px;
}

.personal {
    display: inline-block;
    position: absolute;
    right: 0;
    top: 0;
}

.personal div img {
    width: 26px;
    height: 26px;
}

.screenbox {
    background: #fff;
    border-bottom: 1px solid #d1d1d1;
    position: relative;
    z-index: 999
}

.screening {
    height: 41px;
    line-height: 41px;
}

.screen_left {
    background: url(../images/screen_down.png) no-repeat left center;
    background-size: 18px 15px;
    padding-left: 22px;
    float: left;
    margin-left: 40px;
    min-width: 30px;
    min-height:41px;
}

.screen_right {
    background: url(../images/screen_down.png) no-repeat left center;
    background-size: 18px 15px;
    padding-left: 22px;
    float: right;
    margin-right: 40px;
    min-width: 30px;
    min-height:41px;
}

.screen_up {
    background: url(../images/screen_up.png) no-repeat left center;
    background-size: 18px 15px;
}

.banner {
    position: relative;
    z-index: 999;
}

.banner img {
    width: 100%;
}

.maskbox {
    z-index: 90px;
    position: absolute;
    left: 0;
    top: 44px;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0.7;
    filter: alpha(opacity=70);
}

/*线下门诊列表微信*/
.box_list_mz {
    border-top: 1px solid #d1d1d1;
    margin-top: 9px;
}

.list_mz {
    padding: 10px 0;
    height: 58px;
}

.list_mz .Left {
    float: left;
    width: 58px;
    height: 58px;
    overflow: hidden;
    margin-right: 7px;
}

.list_mz .Left img {
    width: 56px;
    height: 56px;
    border: 1px solid #d1d1d1;
}

.list_mz .Center {
    float: left;
    width: 260px;
}

.list_mz .Center .row_1 {
    font-size: 13px;
    margin-bottom: 5px;
    color: #000;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list_mz .Center .row_2 {
    font-size: 13px;
    color: #919191;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list_mz .Center .row_2 .star {
    width: 13px;
    height: 13px;
    display: inline-block;
    margin-right: 3px;
}

.list_mz .Center .row_2 .star_red {
    background: url(../images/redstar.png) no-repeat left center;
    background-size: 13px 13px;
}

.list_mz .Center .row_2 .star_gray {
    background: url(../images/graystar.png) no-repeat left center;
    background-size: 13px 13px;
}

.list_mz .Center .row_2 strong {
    font-weight: normal;
}

.list_mz .Center .row_3 {
    font-size: 13px;
    color: #919191;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list_mz .Right {
    float: right;
    background: url(../images/arro_right.png) no-repeat left center;
    background-size: 9px 16px;
    display: inline-block;
    width: 9px;
    height: 16px;
    margin-top: 24px;
}

/*线下门诊微信*/
.box_yz {
    margin-top: 9px;
    border-top: 1px solid #d1d1d1;
    border-bottom: 1px solid #d1d1d1;
}

.box_list_yz {
    padding: 0 0 0 11px;
    border-bottom: none;
}

.list_yz {
    padding: 14px 0;
    height: 45px;
    border-bottom: 1px solid #d1d1d1;
}

.bd_none {
    border-bottom: none
}

.list_yz .Left {
    float: left;
    width: 45px;
    height: 45px;
    overflow: hidden;
    margin-right: 20px;
}

.list_yz .Left img {
    width: 43px;
    height: 43px;
    border: 1px solid #d1d1d1;
    border-radius: 22px;
}

.list_yz .Center {
    float: left;
    width: 180px;
}

.list_yz .Center .row_1 {
    font-size: 14px;
    margin-bottom: 0;
    height: 43px;
    line-height: 43px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list_yz .Right {
    float: right;
    background: url(../images/arro_right.png) no-repeat left center;
    background-size: 9px 16px;
    display: inline-block;
    width: 9px;
    height: 16px;
    margin-top: 14px;
    margin-right: 11px;
}

/*医生列表*/
.box_list_ys {
    padding: 0 0 0 11px
}

.box_list_ys {
    border-top: 1px solid #d1d1d1;
    margin-top: 9px;
}

.list_ys {
    padding: 11px 11px 16px 0;
    height: 66px;
    position: relative;
}

.list_ys .Left {
    float: left;
    width: 66px;
    height: 66px;
    overflow: hidden;
    margin-right: 9px;
}

.list_ys .Left img {
    width: 64px;
    height: 64px;
    border: 1px solid #d1d1d1;
    border-radius: 32px;
}

.list_ys .Center {
    width: 100%;
}

.list_ys .Center .row_1 {
    font-size: 13px;
    color: #000;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list_ys .Center .row_2 {
    font-size: 12px;
    color: #919191;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list_ys .Right {
    position: absolute;
    right: 11px;
    top: 11px;
    display: inline-block;
    background: #e27676;
    padding: 2px 5px;
    color: #fff;
    font-size: 12px;
    border-radius: 9px;
}

.address_ys {
    border-top: 1px dashed #e0e0e0;
    padding: 10px 10px 10px 0;
    position: relative
}

.address_ys .Left {
    float: left;
    margin-right: 10px;
    margin-top: -3px;
    width: 15px;
    display: inline-block;
    font-size: 16px;
    color: #919191;
}

.address_ys .Center {
    float: left;
    font-size: 13px;
    color: #000;
    width: 300px;
    line-height: 16px
}

.address_ys .Center .row_1 span {
    padding: 0 5px;
    border-radius: 3px;
    color: #fff;
    background: #fb6787;
    margin-left: 5px;
    font-size: 12px;
}

.address_ys .Right {
    background: url(../images/arro_right.png) no-repeat left center;
    background-size: 9px 16px;
    display: inline-block;
    width: 9px;
    height: 16px;
    float: right;
}

/*医生列表科室与下拉菜单*/
.conditbox {
    position: absolute;
    left: 0;
