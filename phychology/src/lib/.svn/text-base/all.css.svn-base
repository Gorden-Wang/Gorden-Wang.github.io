}

.pjtext textarea::-webkit-input-placeholder {
    color: #919191;
}

/*预约体检成功*/
.sucbox_in .dhicon {
    width: 100px;
    margin: 0 auto;
    height: 90px;
    border-bottom: 1px dotted #d1d1d1
}

.sucbox_in .dhicon img {
    width: 75px;
    height: 75px;
}

.sucbox_in {
    text-align: center;
    background: #fff;
    padding: 20px 11px;
}

.sucbox_in p {
    text-align: center;
    font-size: 14px;
    line-height: 1.8em;
    color: #919191
}

.sucbox_in p a {
    color: #f86987;
    text-decoration: underline
}

.blank {
    width: 100%;
    height: 45px;
}

/*城市微信*/
.citybox {
    width: 100%;
    background: #fff;
    position: absolute;
    left: 0;
    top: 51px;
    padding: 10px 0
}

.citybox ul li {
    width: 33%;
    float: left;
    height: 28px;
    line-height: 30px;
    margin-bottom: 10px;
}

.citybox ul li a {
    display: block;
    margin: 0 10px;
    text-align: center;
    background: #fff;
    color: #000;
    border-radius: 5px;
    box-shadow: 1px 1px 2px 1px #a2a2a2;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.citybox ul li.selected a {
    background: #f86987;
    color: #fff;
}

/*待就诊*/
.secbox {
    margin: 0 11px;
    box-shadow: 0 2px 1px 1px #dbdbdb
}

.box_tit {
    height: 26px;
    line-height: 26px;
    border-bottom: 1px dashed #d1d1d1;
    border-radius: 3px;
    background: #fff;
    color: #919191;
    font-size: 13px;
}

.weiwc {
    padding: 0 7px;
    background: #8be1d4;
    color: #fff;
    display: inline-block;
    border-radius: 3px 0 0 3px;
    margin-right: 3px;
}

.wc {
    padding: 0 7px;
    background: #aac5f4;
    color: #fff;
    display: inline-block;
    border-radius: 3px 0 0 3px;
    margin-right: 3px;
}

.box_tit strong {
    font-weight: normal;
    float: right;
    padding-right: 3px;
}

.oddetail {
    border-radius: 3px;
    background: #fff;
    padding: 10px;
}

.details {
    padding: 5px 0;
}

.details strong {
    font-weight: normal;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-left: 15px;
}

.details .more {
    float: right;
    background: url(../images/arro_right.png) no-repeat left center;
    background-size: 9px 16px;
    display: inline-block;
    width: 9px;
    height: 16px;
    margin-top: 2px;
}

.rules {
    border-bottom: 1px solid #d1d1d1;
    border-top: 1px solid #d1d1d1;
    margin: 17px 0 0 0;
    background: #fff;
}

.rules .titles {
    margin-left: 11px;
    border-bottom: 1px dashed #d1d1d1;
    height: 33px;
    line-height: 33px;
    color: #f86987;
    font-size: 14px;
}

.ruledetail {
    padding: 5px 11px;
}

.ruledetail div {
    line-height: 1.5em;
    color: #666;
    position: relative;
    padding-left: 14px;
}

.ruledetail div span {
    display: inline-block;
    height: 5px;
    width: 5px;
    border-radius: 5px;
    background: #f86987;
    margin-right: 10px;
