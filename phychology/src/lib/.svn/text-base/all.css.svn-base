    padding-left: 36%;
}

.condit span {
    display: inline-block;
    margin-right: 22px;
    float: left
}

.condit .ks_bx {
    background: url(../images/ks_bx.png) no-repeat left center;
    background-size: 19px 18px;
    width: 19px;
    height: 18px;
    float: left
}

.condit .ks_fk {
    background: url(../images/ks_fk.png) no-repeat left center;
    background-size: 19px 17px;
    width: 19px;
    height: 17px;
}

.condit .ks_ck {
    background: url(../images/ks_ck.png) no-repeat left center;
    background-size: 19px 23px;
    width: 19px;
    height: 23px;
}

.condit .ks_ek {
    background: url(../images/ks_ek.png) no-repeat left center;
    background-size: 19px 26px;
    width: 19px;
    height: 26px;
}

.condit .ks_pfk {
    background: url(../images/ks_pfk.png) no-repeat left center;
    background-size: 19px 23px;
    width: 19px;
    height: 23px;
}

.condit .ks_zxk {
    background: url(../images/ks_zxk.png) no-repeat left center;
    background-size: 19px 22px;
    width: 19px;
    height: 22px;
}

.conditbox_ys .condit {
    padding-left: 0;
    text-align: center
}

