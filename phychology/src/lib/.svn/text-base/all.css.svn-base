    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/*个人中心 登录*/
.pinkbox {
    background: #ffb5c6;
    color: #fff;
    font-size: 12px;
    padding: 0 11px;
    height: 32px;
    line-height: 32px;
    text-align: center;
    margin-bottom: 10px;
}

.minlable {
    border-bottom: 1px solid #d1d1d1;
    border-top: 1px solid #d1d1d1;
    padding: 0 16px;
    height: 50px;
    line-height: 50px;
    background: #fff;
    margin-bottom: 10px;
}

.minlable span {
    color: #000;
    font-size: 14px;
    float: left;
    width: 45px;
}

.minlable span select {
    border: none;
    text-align: left
}

.minlable input {
    border: none;
    margin-left: 23px;
    height: 48px;
    line-height: 48px;
    width: 150px;
    font-size: 14px;
    color: #000
}

.minlable input.dh {
    width: 120px;
}

.minlable .hqyzm {
    height: 32px;
    line-height: 32px;
    padding: 0 10px;
    border: 1px solid #ffb5c6;
    color: #ffb5c6;
    border-radius: 8px;
    display: inline-block;
    float: right;
    margin-top: 8px;
}

.minlable .qh {
    height: 38px;
    line-height: 38px;
    border-right: 1px solid #d1d1d1;
    width: 51px;
    margin-top: 7px;
    margin-left: -7px; /*background:url(../images/arro_down.png) no-repeat right center;background-size:16px 9px;*/
}

.submit_btn {
    margin: 0 28px;
}

.submit_btn input {
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #fff;
    background: #fb6c8a;
    border: none;
    letter-spacing: 3px;
    border-radius: 10px;
    margin-top: 10px;
    font-size: 16px;
}

/*服务评价*/
.pjbox {
    margin-top: 54px;
}

.evalua {
    padding: 0 11px;
    height: 50px;
    line-height: 50px;
    margin-bottom: 10px;
    border-bottom: 1px solid #d1d1d1;
    border-top: 1px solid #d1d1d1;
    background: #fff;
}

.evalua strong {
    font-weight: normal;
    font-size: 14px;
    margin-right: 10px;
}

.evalua .star {
    width: 13px;
    height: 13px;
    display: inline-block;
    margin-right: 3px;
}

.evalua .star_red {
    background: url(../images/redstar.png) no-repeat left center;
    background-size: 13px 13px;
}

.evalua .star_gray {
    background: url(../images/graystar.png) no-repeat left center;
    background-size: 13px 13px;
}

.pjtext {
    border-bottom: 1px solid #d1d1d1;
    border-top: 1px solid #d1d1d1;
    padding: 8px 11px;
    background: #fff
}

.textareabox {
    border: 1px solid #d1d1d1;
    border-radius: 5px;
    padding: 7px
}

.pjtext textarea {
    min-height: 140px;
    color: #000;
    width: 100%;
    border: none;
