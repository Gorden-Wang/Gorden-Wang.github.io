.tipsanimated {
  -webkit-animation-duration: 3s;
  animation-duration: 3s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}

@-webkit-keyframes tipsfadeIn {
  0% {
    opacity: 0;
    background-color: black;
    -webkit-animation-timing-function: ease-in;
  }

  35% {
    opacity: 0.8;
    background-color: black;
  }

  65% {
    opacity: 0.8;
    background-color: black;
  }

  100% {
    opacity: 0;
    background-color: black;
    -webkit-animation-timing-function: ease-out;
  }
}

@keyframes tipsfadeIn {
  0% {
    opacity: 0;
    background-color: black;
    -webkit-animation-timing-function: ease-in;
  }

  35% {
    opacity: 0.8;
    background-color: black;
  }

  65% {
    opacity: 0.8;
    background-color: black;
  }

  100% {
    opacity: 0;
    background-color: black;
    -webkit-animation-timing-function: ease-out;
  }
}

.tipsfadeIn {
  -webkit-animation-name: tipsfadeIn;
  animation-name: tipsfadeIn;
}

.fadeAnimate {
  -webkit-animation-duration: 0.3s;
  animation-duration: 0.3s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}

@-webkit-keyframes fadeIn {
  0% {
    opacity: 0;
    display: none;
    -webkit-animation-timing-function: ease-in;
  }
  2% {
    opacity: 0.2;
    height: 100%;
    -webkit-animation-timing-function: ease-in;
  }
  100% {
    opacity: 1;
    height: 100%;
    -webkit-animation-timing-function: ease-out;
  }
}

@keyframes fadeIn {
  0% {
    opacity: 0;
    -webkit-animation-timing-function: ease-in;
  }
  2% {
    opacity: 0.2;
    height: 100%;
    -webkit-animation-timing-function: ease-in;
  }
  100% {
    opacity: 1;
    height: 100%;
    -webkit-animation-timing-function: ease-out;
  }
}

.fadeIn {
  -webkit-animation-name: fadeIn;
  animation-name: fadeIn;
}

.fixed {
  position: fixed;
  top: 45%;
  width: 80%;
  margin: 0 10%;
  color: white;
  padding: 10px;
  font-size: 16px;
  -webkit-box-sizing: border-box;
  -webkit-border-radius: 4px;
  text-align: center;
  z-index: 200;
}
