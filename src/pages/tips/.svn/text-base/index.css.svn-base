#page {
  background-color: white; }
  #page .wrap {
    margin-top: 0px;
    background-color: white;
    margin: 0px 10px;
    overflow: hidden; }
  #page .copy-wrapper {
    text-align: center;
    height: 1px;
    border-top: 1px dashed gray;
    position: absolute;
    margin-top: 40px;
    bottom: 30px;
    width: 100%; }
    #page .copy-wrapper p {
      position: relative;
      top: -10px;
      background-color: white;
      width: 6em;
      margin: auto;
      text-align: center; }
